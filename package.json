{
  "name": "ts-tutorial",
  "version": "1.0.0",
  "description": "TS tutorial",
  "main": "dist/app.js",
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "compile": "cross-env-shell babel src -d dist --source-maps --extensions '.ts'",
    "build": "yarn run clean && yarn run compile",
    "typecheck": "tsc --p .",
    "build-typecheck": "yarn run typecheck && yarn run build",
    "start": "yarn run build-typecheck"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renardelyon/ts-tutorial.git"
  },
  "keywords": [
    "typescript"
  ],
  "author": "Renard Elyon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/renardelyon/ts-tutorial/issues"
  },
  "homepage": "https://github.com/renardelyon/ts-tutorial#readme",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
